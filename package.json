{
  "name": "trustnet-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:dev": "ts-node-dev ./src/app.ts",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "start": "node ./dist/app.js",
    "test": "npm run db:reset:test && jest --detectOpenHandles",
    "db:reset": "ts-node-dev ./src/scripts/resetDb.script.ts",
    "db:create": "ts-node-dev ./src/scripts/createDb.script.ts",
    "db:seed": "ts-node-dev ./src/scripts/createSeed.script.ts",
    "db:reset:test": "NODE_ENV=test ts-node-dev ./src/scripts/resetDb.script.ts",
    "db:migrate": "node-pg-migrate --migrations-dir='./src/db/migrations/public' --tsconfig=./tsconfig.json --migrations-table='schema_migrations'"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bcrypt-nodejs": "^0.0.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.1.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "node-pg-migrate": "^6.2.1",
    "passport": "^0.6.0",
    "passport-azure-ad": "^4.3.3",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "^0.0.31",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.41",
    "@types/passport-azure-ad": "^4.3.1",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.2"
  }
}