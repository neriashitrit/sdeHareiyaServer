{
  "name": "safe-shore-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:dev": "ts-node-dev ./src/app.ts",
    "start:prod": "rm -rf dist && tsc && node ./dist/app.js",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "start": "node ./dist/app.js",
    "db:create": "ts-node-dev ./src/scripts/createDb.script.ts",
    "db:seed": "ts-node-dev ./src/scripts/createSeed.script.ts",
    "db:migrate": "node-pg-migrate --migrations-dir='./src/db/migrations' --tsconfig=./tsconfig.json --migrations-table='schema_migrations'",
    "db:migrate:up": "node-pg-migrate up --no-check-order --migrations-dir='./src/db/migrations' --tsconfig=./tsconfig.json --migrations-table='schema_migrations'",
    "db:migrate:create": "node-pg-migrate create --migrations-dir='./src/db/migrations' --tsconfig='./tsconfig.json' --migrations-table='schema_migrations'",
    "db:migrate:down": "node-pg-migrate down --no-check-order --migrations-dir='./src/db/migrations' --tsconfig=./tsconfig.json --migrations-table='schema_migrations'",
    "db:reset": "ts-node ./src/scripts/resetDb.ts"
  },
  "dependencies": {
    "@azure/storage-file-share": "^12.11.0",
    "@sendgrid/mail": "^7.7.0",
    "axios": "^0.27.2",
    "azure-connection-parser": "^1.0.1",
    "azure-storage": "^2.10.4",
    "bcrypt-nodejs": "^0.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "knex": "^2.1.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "node-pg-migrate": "^6.2.1",
    "objection": "^3.0.1",
    "passport": "^0.6.0",
    "passport-azure-ad": "^4.3.3",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "^0.0.31",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.4.1",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.41",
    "@types/passport-azure-ad": "^4.3.1",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express-fileupload": "^1.3.1",
    "prettier": "^2.6.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.2"
  }
}